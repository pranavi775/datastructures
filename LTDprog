#assigning elements to different lists

list1=[1,2,3]
list1.append(1)      #append()- inserts element at last
print("list when u appent element",list1)
list1.insert(2, 1)   #insert(indexnum, value)- inserts element at specified index
print("list when u insert element",list1)

#accessing elements from a tuple

my_tuple=(5,6,7,8,9)
print("the element is :",my_tuple[0])

#deleting different dictionary elements

my_dic={1:"a",2:"b",3:"c"}
#print(my_dic)
del my_dic[3]
print(my_dic)
